# Suppress webpack dev server deprecation warnings
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true

# Suppress specific Node.js deprecation warnings
NODE_OPTIONS="--no-deprecation"

# Aggressive performance optimizations for faster startup
FAST_REFRESH=true
SKIP_PREFLIGHT_CHECK=true
TSC_COMPILE_ON_ERROR=true
ESLINT_NO_DEV_ERRORS=true
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=3000

# Disable slow features during development
DISABLE_NEW_JSX_TRANSFORM=false
IMAGE_INLINE_SIZE_LIMIT=0

# Webpack performance optimizations
WEBPACK_HIDE_WARNINGS=true
WEBPACK_DISABLE_HOST_CHECK=true
WEBPACK_USE_POLLING=false

# TypeScript optimizations
TS_NODE_TRANSPILE_ONLY=true
TS_NODE_SKIP_PROJECT=true

# Disable expensive checks
DISABLE_TYPE_CHECKING=true
DISABLE_ESLINT=true