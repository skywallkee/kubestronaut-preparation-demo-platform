    "resources": [
      "pods"
    ],
    "prerequisites": []
  },
  "solution": {
    "steps": [
      "1. kubectl annotate pod nginx1 --list",
      "2. kubectl describe po nginx1 | grep -i 'annotations'",
      "3. kubectl get po nginx1 -o custom-columns=Name:metadata.name,ANNOTATIONS:metadata.annotations.description"
    ]
  },
